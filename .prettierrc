{
  "printWidth": 3500,
  "tabWidth": 2,
  "trailingComma": "es5",
  "proseWrap": "always",
  "endOfLine": "auto",
  "overrides": [
    { "files": "*.ts", "options": { "parser": "typescript" } },
    { "files": "*.html", "options": { "parser": "html" } },
    { "files": "*.vue", "options": { "parser": "vue" } },
    { "files": "*.less", "options": { "parser": "less" } },
    { "files": "*.json", "options": { "parser": "json" } }
  ]
}
